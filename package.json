{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "start": "next start"
  },
  "next-unused": {
    "alias": {
      "@styles/*": [
        "styles/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@providers/*": [
        "providers/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@takeshape/routing": "^7.38.2",
    "@types/node": "^15.0.1",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "js-cookie": "^2.2.1",
    "next": "10.1.3",
    "next-seo": "^4.24.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "7.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-merge-refs": "^1.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.1.3",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.4",
    "autoprefixer": "^10.2.5",
    "next-unused": "0.0.6",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.4"
  },
  "license": "MIT"
}
